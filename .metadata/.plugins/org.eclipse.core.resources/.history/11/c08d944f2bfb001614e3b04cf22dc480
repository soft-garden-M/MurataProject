#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int begin = 10;
	int end = 10;
	int sum = 0;
	sum = count(begin,end);
	printf("%d",sum);
	return EXIT_SUCCESS;
}

int count(int begin,int end){
	int sum = 0;
	if (begin < end){
		for (begin;begin <= end ; begin++){
			sum = sum + begin;
		}
		return sum;
	}else{
		return -1;
	}
}
