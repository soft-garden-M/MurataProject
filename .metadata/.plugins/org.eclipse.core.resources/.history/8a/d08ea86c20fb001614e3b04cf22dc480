	int end = 10;
	int sum = 0;
	//int i;
	//int count = 0;
	//for(i=0;i<=10;i = i+1){
	//	count = count + i;
	//}
	count(begin,end,sum);
	printf("%d",count);
	return EXIT_SUCCESS;
}

int count(int begin,int end){
	sum = 0;
	sum = begin + end;
	return count;
}
