int main(void){
    int toshi = 2017;
    int uruu = 5;
    int c;
    char hantei[1];

    if (c = 0){
    	printf("%d\n","うるう年です。");
    }else{
    	printf("%d\n","うるう年ではありません。");
    }

    printf("%d\n",hantei[0]); /*余りは『1』*/

}

int uruuhantei(int toshi,int uruu){
    int  = toshi % uruu;

	return c;

}

