int main(void){
    #define ADD 0
    #define SUB int 1
    #define MUL int 2
    #define DIVI int 3

	int a;
	a = 2;
	int b;
	b = 4;
	int c;

	c = keisan(a,b,ADD);
	printf("%d",c);


}


int keisan(int suuji1,int suuji2, define ADD){
	int kekka;
	kekka = suuji1 + suuji2;

	return kekka;

}
